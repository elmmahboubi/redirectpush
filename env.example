# GoLinks Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Superuser Authentication (REQUIRED for security)
VITE_SUPERUSER_USERNAME=your_username_here
VITE_SUPERUSER_PASSWORD=your_secure_password_here

# Base URL for short links (optional)
VITE_BASE_URL=https://your-domain.com
